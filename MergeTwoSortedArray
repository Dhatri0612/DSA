package MergeTwoSortedrray;

import java.util.Arrays;

public class main {
    public static void main(String[] args) {
        int n1[]={2,1,5,4};
        int n2[]={ 6,7,9,8};
        int n3[] = new int[n1.length + n2.length];
        for(int i=0;i< n1.length;i++){
            n3[i] = n1[i];
        }
        for(int j=0;j< n2.length;j++){
            n3[n1.length + j] = n2[j];
        }
        Arrays.sort(n3);
        System.out.print(Arrays.toString(n3));
    }
}
